<div class= "bottom_right" id="index_bottom_right">
  <div class="black scroll_pane" id="BRL">
    <h2 class="box_title">Announcements</h2>
    <% @announcements.each do |f| %>
    <div class = "Announcement" >
      <h2><%= f.name %></h2>
      <p class="description"><%= f.description.html_safe %></p>
    </div>
    <% end %>
  </div>
  <%= render "themes/avec/events/box" %>
</div>
